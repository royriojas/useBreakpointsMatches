{
  "name": "use-breakpoint-matches",
  "version": "1.0.0",
  "description": "Simple i18n helper for key/value access",
  "main": "dist/use-breakpoint-matches.js",
  "types": "dist/use-breakpoint-matches.d.ts",
  "license": "MIT",
  "author": "Roy Riojas",
  "keywords": [
    "responsive helpers",
    "breakpoints",
    "css responsive design"
  ],
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "TZ=UTC jest",
    "lint": "eslint --cache --cache-location=node_modules/.cache/ ./src/ ./__tests__/ --ext .ts,.tsx,.js",
    "check:types": "tsc",
    "verify": "yarn lint && yarn check:types",
    "autofix": "yarn lint --fix",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx,.js\" --source-maps inline",
    "prerelease": "npm run build",
    "release": "semantic-release",
    "release:preflight": "npm pack --dry-run"
  },
  "devDependencies": {
    "@babel/cli": "7.16.0",
    "@babel/core": "7.16.0",
    "@babel/plugin-transform-runtime": "7.16.0",
    "@babel/preset-env": "7.16.4",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "7.16.0",
    "@babel/register": "7.16.0",
    "@semantic-release/changelog": "6.0.0",
    "@semantic-release/git": "10.0.0",
    "@semantic-release/gitlab": "7.0.3",
    "@semantic-release/npm": "8.0.0",
    "@types/eslint": "^7.2.5",
    "@types/jest": "27.0.2",
    "@types/node": "17.0.14",
    "@types/pino": "6.3.6",
    "@types/react": "17.0.38",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "babel-jest": "27.4.6",
    "eslint": "^7.14.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jest-playwright": "^0.2.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "27.4.7",
    "playwright-core": "1.9.0",
    "prettier": "2.4.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "semantic-release": "18.0.0",
    "tslib": "2.3.1",
    "typescript": "4.4.4"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "dependencies": {
    "@babel/runtime": "7.16.3"
  }
